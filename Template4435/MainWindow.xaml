<Window x:Class="Template4435.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Template4435"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="800" MinHeight="800" MinWidth="400">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="23*"/>
            <ColumnDefinition Width="57*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>

            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>

        </Grid.RowDefinitions>

        <Button x:Name="BnTask" Click="BnTask_Click" Content="Задача" Grid.Row="14" Grid.ColumnSpan="2" Margin="0,0,0,29" Grid.RowSpan="2"></Button>
        <Button x:Name="toWindowBtn" Click="toWindowBtn_Click" Content="Мартынов Максим Задача" Grid.Row="8" Grid.ColumnSpan="2" Margin="0,29,0,0" Grid.RowSpan="2"></Button>
        <Button x:Name="AzatBtn" Click="AzatBtn_Click"  Content="Задача 23" Grid.Row="23" Grid.ColumnSpan="2" Margin="0,0,0,29" Grid.RowSpan="2"></Button>
        <Button x:Name="LR1_Shumilkin" Click="LR1_Shumilkin_Click" Content="КОНЕЦ! Шумилкин А.О." Grid.Row="15" Grid.ColumnSpan="2" Margin="0,0,0,29" Grid.RowSpan="2" />
        <Button x:Name="BnnTask" Click="BnnTask_Click" Content="Задача" Grid.Row="11" Grid.ColumnSpan="2" Margin="0,29,0,29" Grid.RowSpan="3"></Button>
        <StackPanel  Grid.Row="3" Orientation="Horizontal" Grid.ColumnSpan="2" Margin="0,29,0,0" Grid.RowSpan="2">
            <Button x:Name="BtnCHELNY" Click="BtnCHELNY_Click"   Content="ЧЕЛНЫ ТОП " Margin="10 0"></Button>
            <Button x:Name="BtnCHELNYImport" Click="BtnCHELNYImport_Click" Content="ЧЕЛНЫ ТОП Импорт" Margin="10 0"></Button>
            <Button x:Name="BtnCHELNYExport" Click="BtnCHELNYExport_Click" Content="ЧЕЛНЫ ТОП Экспорт" Margin="10 0"></Button>
            <Button x:Name="BtnCHELNYImportJSON" Click="BtnCHELNYImportJSON_Click" Content="ЧЕЛНЫ ТОП Импорт JSON" Margin="10 0"></Button>
            <Button x:Name="BtnCHELNYExportJSON" Click="BtnCHELNYExportJSON_Click" Content="ЧЕЛНЫ ТОП Экспорт DOCX" Margin="10 0"></Button>
        </StackPanel>

        <Button x:Name="BtnNikita" Click="BtnNikita_Click" Content="RHFDXTYRJ" Grid.Row="5" Grid.ColumnSpan="2" Margin="0,29,0,0" Grid.RowSpan="2"></Button>
    </Grid>
</Window>